{
  "name": "generator-afl",
  "version": "0.0.0",
  "description": "Offical AngularFire Lite CLI",
  "homepage": "https://github.com/hamedbaatour/angularfire-lite-cli",
  "author": {
    "name": "Hamed Baatour",
    "email": "hamedbaatour@gmail.com",
    "url": "https://github.com/hamedbaatour"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "firebase generator",
    "firebase cli",
    "angularfire lite",
    "angularfire lite generator",
    "firebase angular generator",
    "firebase tempates generator",
    "angularfire",
    "firebase with angular",
    "angularfire lite cli",
    "angularfire cli",
    "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "nsp": "^3.2.1",
    "eslint": "^5.1.0",
    "prettier": "^1.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-config-xo": "^0.23.0",
    "jest": "^23.4.1",
    "jest-cli": "^23.4.1"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "log-symbols": "^2.1.0",
    "yeoman-generator": "^3.0.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "hamedbaatour/generator-afl",
  "license": "Apache-2.0"
}
